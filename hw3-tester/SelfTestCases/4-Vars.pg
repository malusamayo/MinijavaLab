MAIN
	PRINT CALL
	BEGIN
		MOVE TEMP 20 BEGIN
			MOVE TEMP 22 HALLOCATE 4
			MOVE TEMP 23 HALLOCATE 8
			HSTORE TEMP 22 0 TEMP 23
			HSTORE TEMP 23 0 A_helper
			HSTORE TEMP 23 4 A_run

			RETURN TEMP 22

		END
		HLOAD TEMP 21 TEMP 20 0
		HLOAD TEMP 21 TEMP 21 4

		RETURN TEMP 21

	END
	(TEMP 20 )

END

A_run [ 1 ]
	BEGIN
		MOVE TEMP 20 CALL
		BEGIN
			MOVE TEMP 22 TEMP 0 
			HLOAD TEMP 23 TEMP 22 0
			HLOAD TEMP 23 TEMP 23 0

			RETURN TEMP 23

		END
		(TEMP 22 12 )
		MOVE TEMP 21 CALL
		BEGIN
			MOVE TEMP 24 TEMP 0 
			HLOAD TEMP 25 TEMP 24 0
			HLOAD TEMP 25 TEMP 25 0

			RETURN TEMP 25

		END
		(TEMP 24 15 )

		RETURN PLUS TEMP 20  TEMP 21 

	END
	A_helper [ 2 ]
		BEGIN
			MOVE TEMP 20 TEMP 1 			MOVE TEMP 1 PLUS TEMP 1  1 			PRINT TEMP 20 
			RETURN TEMP 20 

		END
